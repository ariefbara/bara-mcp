<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping>
    <entity name="Payment\Domain\Model\Firm\Team" 
            table="Team"
    >
        
        <id name="id" type="guid" />
        <field name="name" type="string" length="255" nullable="false"/>
        
        <many-to-one  field="creator" 
                      target-entity="Payment\Domain\Model\Firm\Client"
        >
            <join-column name="Client_idOfCreator" referenced-column-name="id" />
        </many-to-one>
        
    </entity>
</doctrine-mapping>
