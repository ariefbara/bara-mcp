<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping>
    <entity name="Query\Domain\Model\Shared\FileInfo" 
            table="FileInfo" 
    >
        <id name="id" type="guid" />
        <field name="folders" type="simple_array" nullable="true"/>
        <field name="name" type="string" length="256" nullable="false"/>
        <field name="size" type="float" nullable="true"/>
        
    </entity>
</doctrine-mapping>
