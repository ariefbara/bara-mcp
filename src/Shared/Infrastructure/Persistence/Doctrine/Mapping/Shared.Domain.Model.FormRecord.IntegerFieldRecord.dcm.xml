<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping>
    <entity name="Shared\Domain\Model\FormRecord\IntegerFieldRecord" 
            table="IntegerFieldRecord" 
    >
        
        <many-to-one  field="formRecord" 
                      target-entity="Shared\Domain\Model\FormRecord"
                      inversed-by="integerFieldRecords"
        >
            <join-column name="FormRecord_id" referenced-column-name="id" />
        </many-to-one>
        <id name="id" type="guid" />
        <many-to-one  field="integerField" 
                      target-entity="Shared\Domain\Model\Form\IntegerField"
        >
            <join-column name="IntegerField_id" referenced-column-name="id" />
        </many-to-one>
        <field name="value" type="integer" nullable="true"/>
        <field name="removed" type="boolean" />
        
    </entity>
</doctrine-mapping>
